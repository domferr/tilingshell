#: dist/prefs.js:783
msgid "General"
msgstr "Obecné"

#: dist/prefs.js:788
msgid "Appearance"
msgstr "Vzhled"

#: dist/prefs.js:789
msgid "Configure the appearance of Tiling Shell"
msgstr "Nastavení vzhledu Tiling Shell"

#: dist/prefs.js:794
msgid "Show Indicator"
msgstr "Zobrazit ikonu"

#: dist/prefs.js:795
msgid "Whether to show the panel indicator"
msgstr "Zda se má zobrazit ikona na panelu"

#: dist/prefs.js:800
msgid "Inner gaps"
msgstr "Vnitřní mezery"

#: dist/prefs.js:801
msgid "Gaps between windows"
msgstr "Mezery mezi okny"

#: dist/prefs.js:806
msgid "Outer gaps"
msgstr "Vnější mezery"

#: dist/prefs.js:807
msgid "Gaps between a window and the monitor borders"
msgstr "Mezery mezi oknem a okrajem monitoru"

#: dist/prefs.js:811
msgid "Blur (experimental feature)"
msgstr "Rozostření (experimentální funkce)"

#: dist/prefs.js:813
msgid "Apply blur effect to Snap Assistant and tile previews"
msgstr "Použít efekt rozostření na asistenta přichycení a náhledy dlaždic"

#: dist/prefs.js:819
msgid "Snap Assistant threshold"
msgstr "Vzdálenost asistenta přichycení"

#: dist/prefs.js:821
msgid "Minimum distance from the Snap Assistant to the pointer to open it"
msgstr ""
"Minimální vzdálenost od asistenta přichycení k ukazateli pro jeho otevření"

#: dist/prefs.js:830
msgid "Snap Assistant"
msgstr "Asistent přichycení"

#: dist/prefs.js:831
msgid "Apply blur effect to Snap Assistant"
msgstr "Použít efekt rozostření na asistenta přichycení"

#: dist/prefs.js:837
msgid "Selected tile preview"
msgstr "Náhled vybrané dlaždice"

#: dist/prefs.js:838
msgid "Apply blur effect to selected tile preview"
msgstr "Použít efekt rozostření na vybraný náhled dlaždice"

#: dist/prefs.js:842
msgid "Window border"
msgstr "Ohraničení okna"

#: dist/prefs.js:843 dist/prefs.js:850
msgid "Show a border around focused window"
msgstr "Zobrazení ohraničení kolem vybraného okna"

#: dist/prefs.js:849
msgid "Enable"
msgstr "Povolit"

#: dist/prefs.js:856
msgid "Smart border radius"
msgstr "Inteligentní poloměr ohraničení"

#: dist/prefs.js:857
msgid "Dynamically adapt to the window’s actual border radius"
msgstr "Dynamicky se přizpůsobí skutečnému poloměru okraje okna"

#: dist/prefs.js:863
msgid "Width"
msgstr "Šířka"

#: dist/prefs.js:864
msgid "The size of the border"
msgstr "Velikost ohraničení"

#: dist/prefs.js:870
msgid "Border color"
msgstr "Barva ohraničení"

#: dist/prefs.js:871
msgid "Choose the color of the border"
msgstr "Vyberte barvu ohraničení"

#: dist/prefs.js:877
msgid "Animations"
msgstr "Animace"

#: dist/prefs.js:878
msgid "Customize animations"
msgstr "Přizpůsobení animací"

#: dist/prefs.js:884
msgid "Snap assistant animation time"
msgstr "Doba animace asistenta přichycení"

#: dist/prefs.js:885
msgid "The snap assistant animation time in milliseconds"
msgstr "Doba animace v milisekundách"

#: dist/prefs.js:893
msgid "Tiles animation time"
msgstr "Doba animace dlaždic"

#: dist/prefs.js:894
msgid "The tiles animation time in milliseconds"
msgstr "Doba animace v milisekundách"

#: dist/prefs.js:900
msgid "Behaviour"
msgstr "Chování"

#: dist/prefs.js:901
msgid "Configure the behaviour of Tiling Shell"
msgstr "Nastavení chování Tiling Shell"

#: dist/prefs.js:906
msgid "Enable Snap Assistant"
msgstr "Povolit asistenta přichycení"

#: dist/prefs.js:907
msgid "Move the window on top of the screen to snap assist it"
msgstr ""
"Přesunutím okna k horní části obrazovky mužete vybrat pozici přichycení"

#: dist/prefs.js:912
msgid "Enable Tiling System"
msgstr "Povolit systém dlaždic"

#: dist/prefs.js:913
msgid "Hold the activation key while moving a window to tile it"
msgstr "Podržte aktivační klávesu a přesouvejte okno pro přichycení k dláždici"

#: dist/prefs.js:921
msgid "Tiling System deactivation key"
msgstr "Klávesa pro deaktivaci systému dlaždic"

#: dist/prefs.js:923
msgid ""
"Hold the deactivation key while moving a window to deactivate the tiling "
"system"
msgstr ""
"Podržte deaktivační klávesu při pohybu okna pro deaktivaci systému dláždic"

#: dist/prefs.js:931
msgid "Span multiple tiles"
msgstr "Rozvržení přes více dlaždic"

#: dist/prefs.js:932
msgid "Hold the activation key to span multiple tiles"
msgstr "Podržte aktivační klávesu, abyste překlenuli více dlaždic"

#: dist/prefs.js:941
msgid "Enable Auto Tiling"
msgstr "Povolit automatické dlaždice"

#: dist/prefs.js:942
msgid "Automatically tile new windows to the best tile"
msgstr "Automaticky vložte nová okna na nejvhodnějsí dlaždice"

#: dist/prefs.js:947
msgid "Enable auto-resize of the complementing tiled windows"
msgstr "Povolit automatickou změnu velikosti doplňujících dlaždicových oken"

#: dist/prefs.js:949
msgid ""
"When a tiled window is resized, auto-resize the other tiled windows near it"
msgstr ""
"Když se změní velikost dlaždicového okna, automaticky změníte velikost "
"ostatních dlaždicových oken poblíž"

#: dist/prefs.js:955
msgid "Restore window size"
msgstr "Obnovit velikost okna"

#: dist/prefs.js:957
msgid "Whether to restore the windows to their original size when untiled"
msgstr "Zda vrátit okna do původní velikosti, když nejsou v dlaždicích"

#: dist/prefs.js:963
msgid "Add snap assistant and auto-tile buttons to window menu"
msgstr ""
"Přidat do nabídky okna asistenta přichycení a tlačítka automatického "
"rozložení"

#: dist/prefs.js:965
msgid ""
"Add snap assistant and auto-tile buttons in the menu that shows up when you "
"right click on a window title"
msgstr ""
"Když kliknete pravým tlačítkem myši na název okna, zobrazí se v nabídce "
"tlačítka asistenta přichycení a automatického rozložení"

#: dist/prefs.js:970
msgid "Screen Edges"
msgstr "Okraje obrazovky"

#: dist/prefs.js:972
msgid ""
"Drag windows against the top, left and right screen edges to resize them"
msgstr ""
"Přetažením oken proti hornímu, levému a pravému okraji obrazovky změníte "
"jejich velikost"

#: dist/prefs.js:986
msgid "Drag against top edge to maximize window"
msgstr "Tažením proti hornímu okraji okno maximalizujete"

#: dist/prefs.js:987
msgid "Drag windows against the top edge to maximize them"
msgstr "Přetáhněte okna proti hornímu okraji, abyste je maximalizovali"

#: dist/prefs.js:996
msgid "Quarter tiling activation area"
msgstr "Plocha pro aktivaci dláždic"

#: dist/prefs.js:997
#, javascript-format
msgid "Activation area to trigger quarter tiling (%% of the screen)"
msgstr "Aktivační oblast pro spuštění dláždic (%% obrazovky)"

#: dist/prefs.js:1014
msgid "Windows suggestions"
msgstr "Návrhy oken"

#: dist/prefs.js:1015
msgid "Enable and disable windows suggestions"
msgstr "Povolit a zakázat návrhy oken"

#: dist/prefs.js:1020
msgid "Enable window suggestions for the tiling system"
msgstr "Povolit návrhy oken pro systém dlaždic"

#: dist/prefs.js:1022
msgid ""
"Provides smart suggestions to fill empty tiles when using the tiling system"
msgstr ""
"Poskytuje chytré návrhy, jak vyplnit prázdné dlaždice při použití systému "
"obkladů"

#: dist/prefs.js:1028
msgid "Enable window suggestions for the snap assistant"
msgstr "Povolit návrhy oken pro asistenta přichycení"

#: dist/prefs.js:1030
msgid ""
"Offers suggestions to populate empty tiles when using the snap assistant"
msgstr ""
"Nabízí návrhy k vyplnění prázdných dlaždic při použití asistenta přichycení"

#: dist/prefs.js:1036
msgid "Enable window suggestions for screen edge snapping"
msgstr "Povolit návrhy oken pro přichycení okrajů obrazovky"

#: dist/prefs.js:1038
msgid "Suggests windows to occupy empty tiles when snapping to screen edges"
msgstr ""
"Navrhuje, aby okna obsadila prázdné dlaždice při přichycení k okrajům "
"obrazovky"

#: dist/prefs.js:1044
msgid "Layouts"
msgstr "Rozvržení"

#: dist/prefs.js:1045
msgid "Configure the layouts of Tiling Shell"
msgstr "Nastavení rozvržení Tiling Shell"

#: dist/prefs.js:1049 dist/prefs.js:1050
msgid "Edit layouts"
msgstr "Upravit rozvržení"

#: dist/prefs.js:1051
msgid "Open the layouts editor"
msgstr "Otevřete editor rozvržení"

#: dist/prefs.js:1056 dist/prefs.js:1057 dist/prefs.js:1061
msgid "Export layouts"
msgstr "Exportovat rozvržení"

#: dist/prefs.js:1058
msgid "Export layouts to a file"
msgstr "Exportujte rozvržení do souboru"

#: dist/prefs.js:1064 dist/prefs.js:1435
msgid "Save"
msgstr "Uložit"

#: dist/prefs.js:1065 dist/prefs.js:1120 dist/prefs.js:1436 dist/prefs.js:1492
msgid "Cancel"
msgstr "Zrušit"

#: dist/prefs.js:1111 dist/prefs.js:1112
msgid "Import layouts"
msgstr "Importovat rozvržení"

#: dist/prefs.js:1113
msgid "Import layouts from a file"
msgstr "Importujte rozvržení ze souboru"

#: dist/prefs.js:1116
msgid "Select layouts file"
msgstr "Vyberte soubor rozvržení"

#: dist/prefs.js:1119 dist/prefs.js:1491
msgid "Open"
msgstr "Otevřít"

#: dist/prefs.js:1167 dist/prefs.js:1168
msgid "Reset layouts"
msgstr "Resetovat rozvržení"

#: dist/prefs.js:1169
msgid "Bring back the default layouts"
msgstr "Obnovte výchozí rozvržení"

#: dist/prefs.js:1182
msgid "Keybindings"
msgstr "Klávesové zkratky"

#: dist/prefs.js:1184
msgid "Use hotkeys to perform actions on the focused window"
msgstr "Pomocí klávesových zkratek provádějte akce na vybraném okně"

#: dist/prefs.js:1202
msgid "Move window to right tile"
msgstr "Přesunout okno na pravou dlaždici"

#: dist/prefs.js:1204
msgid "Move the focused window to the tile on its right"
msgstr "Přesuňte vybrané okno na dlaždici napravo"

#: dist/prefs.js:1213
msgid "Move window to left tile"
