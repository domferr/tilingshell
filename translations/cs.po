#: dist/prefs.js:1228
msgid "Move the focused window to the tile below"
msgstr "Přesuňte vybrané okno na dlaždici níže"

#: dist/prefs.js:1234
msgid "Span window to right tile"
msgstr "Roztáhnout okno na pravou dlaždici"

#: dist/prefs.js:1235
msgid "Span the focused window to the tile on its right"
msgstr "Roztáhněte vybrané okno na dlaždici napravo"

#: dist/prefs.js:1241
msgid "Span window to left tile"
msgstr "Roztáhnout okno na levou dlaždici"

#: dist/prefs.js:1242
msgid "Span the focused window to the tile on its left"
msgstr "Roztáhněte vybrané okno na dlaždici nalevo"

#: dist/prefs.js:1248
msgid "Span window above"
