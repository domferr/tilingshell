#: dist/prefs.js:1214
msgid "Move the focused window to the tile on its left"
msgstr "Přesuňte vybrané okno na dlaždici nalevo"

#: dist/prefs.js:1220
msgid "Move window to tile above"
msgstr "Přesunout okno na dlaždici výše"

#: dist/prefs.js:1221
msgid "Move the focused window to the tile above"
msgstr "Přesuňte vybrané okno na dlaždici výše"

#: dist/prefs.js:1227
